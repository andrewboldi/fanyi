{
    "manifest_version": 3,
    "name": "Fanyi: Contextual Translation",
    "version": "0.0.1",
    "action": {
        "default_title": "Fanyi: Contextual Translation"
    },
    "description": "Select a word on the webpage and open popup to instantly look up in the dictionary",
    "author": "us",
    "content_scripts": [
        {
          "matches": [
            "<all_urls>"
          ],
          "js": ["content.js"]
        }
      ],
      "permissions": [
        "activeTab"
      ]
}
